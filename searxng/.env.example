# KhaosSearch Environment Variables
# Copy this file to .env and configure with your values

# SearXNG Secret Key
# Generate with: openssl rand -hex 32
SEARXNG_SECRET=changeme-generate-random-secret-here

# Base URL for your deployment
# Update this with your actual domain
SEARXNG_BASE_URL=https://search.strategickhaos.ai

# Traefik Dashboard Authentication
# Generate with: htpasswd -nb admin yourpassword
# Example: htpasswd -nb admin secretpassword
# Result: admin:$apr1$xyz123$...
TRAEFIK_BASIC_AUTH=

# Email for Let's Encrypt certificates
LETSENCRYPT_EMAIL=security@strategickhaos.ai

# Optional: Tor Configuration
TOR_ENABLED=true
TOR_MAX_CIRCUIT_DIRTINESS=60
TOR_NEW_CIRCUIT_PERIOD=30

# Optional: Redis Configuration
REDIS_PASSWORD=
REDIS_MAX_MEMORY=512mb

# Optional: Performance Tuning
SEARXNG_POOL_CONNECTIONS=100
SEARXNG_POOL_MAXSIZE=20
SEARXNG_REQUEST_TIMEOUT=10.0
